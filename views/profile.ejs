      <div class="collapsible-body black">
<button type="button" class="waves-effect waves-light btn black"><i class="material-icons left">album</i><a href="/music">Add Artists</a></button>
<% favorites.forEach(function(item) { %>
<div class="row">
	<div class="col s12 m5">
		<div class="card-panel grey">
			<span class="white-text"><h7><%= item.artist %></h7>
			</span>
		</div>
	</div>
</div>
<% }); %></p></div>
    </li>
    <li>
      <div class="collapsible-header black"><i class="material-icons">place</i>Pitchfork Top Albums</div>
      <div class="collapsible-body black"><p>
		<h7><% urls.forEach(function(url, index){%>
		<a href="http://pitchfork.com<%= url %>"><%= info[index] %></a></h7>
	</p>

<% }) %>
</span>
</div></p></div>
    </li>
  </ul>

